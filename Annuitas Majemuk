AnnuitasAkhirAwalma <- function(num, k, i, t,m=TRUE)
  switch(num, 
         satu= {
           j = i/m
           n = t*m
           v = 1/(1+j)
           anAwal= k*(1-v^n)/(j*v)
           SnAwal= k*(((1+j)^n)-1)/(j*v)
           print(anAwal)
           print(SnAwal)
         },
         dua= {
           j = i/m
           n = t*m
           v = 1/(1+j)
           anAkhir= k*(1-v^n)/j
           SnAkhir= k*((1+j)^n-1)/j
           print(anAkhir)
           print(SnAkhir)
         }
  )
